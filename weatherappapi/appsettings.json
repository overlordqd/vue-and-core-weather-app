{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AerisWeather": {
    "ApiAddress": "https://api.aerisapi.com",
    "ClientId": "*",
    "ClientSecret": "*",
    "Queries": {
      "Forecast": "/forecasts/{location}?client_secret={client_secret}&client_id={client_id}&fields=periods.dateTimeISO,periods.maxTempC,periods.minTempC,periods.maxHumidity,periods.minHumidity,periods.windSpeedMaxKPH,periods.windSpeedMinKPH,periods.weather&limit=6&filter=day&format=json",
      "Current": "/observations/{location}?client_secret={client_secret}&client_id={client_id}&fields=ob.tempC,ob.humidity,ob.weather,ob.windSpeedKPH&limit=1&filter=allstations&format=json"
    }
  }
}